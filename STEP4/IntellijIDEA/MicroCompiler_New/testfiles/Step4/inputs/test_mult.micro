PROGRAM expr
BEGIN

	INT a,b,c,d,e;
	STRING newline := "\n";

	FUNCTION VOID main()
	BEGIN
		a := 1;
		b := 2;

          READ(c, d);

          e := a * c + b * d;
          e := c - b + a * a / c - b / d * a;
          a := (b / c + b) + (a * a / c) - b / d * a;

          WRITE(c, newline, d, newline, e, newline);

	END	
END
